const readline = require('readline')
let firstNumber, secondNumber, thirdNumber
const rl = readline.createInterface({
    input: process.stdin,
    output: process.stdout
})


rl.question('Insira o primeiro número', (answer) => {
    firstNumber = answer
    rl.close()
})

rl.question('Insira o segundo número', (answer) => {
    secondNumber = answer
    rl.close()
})

rl.question('Insira o terceiro número', (answer) => {
    thirdNumber = answer
    rl.close()
})

console.log(firstNumber)
console.log(secondNumber)
console.log(thirdNumber)